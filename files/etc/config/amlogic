config amlogic 'config'
	option enable '1'
	option amlogic_firmware_repo 'https://github.com/daydreams1231/N1-OpenWrt'
	option amlogic_firmware_tag 'v'
	option amlogic_firmware_suffix '.img.gz'
	option amlogic_kernel_path 'https://github.com/ophub/kernel'
	option amlogic_kernel_branch '6.12'
	option amlogic_firmware_config '1'
	option amlogic_write_bootloader '1'
	option amlogic_shared_fstype 'f2fs'

config settings 'armcpu'
	option governor0 'schedutil'
	option minfreq0 '667000'
	option maxfreq0 '1512000'
